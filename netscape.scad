module tetris_base(h1){
    intersection() {
        translate([0,0,0])
            cube([l, l, d+h1*hunit]);
        translate([r, r, -d*0.01]){
            cylinder(h=(d+h1*hunit)*1.02, d=r*1.6/sin(atan(1/sqrt(2)))-r/22);
        }
    }    
}


module tetris_plate(level, h1){
    o1=(l-l2)/2;
    translate([o1,o1,d+h1*hunit+level*t])
        cube([l2, l2, t]);
}

